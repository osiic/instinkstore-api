{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "category",
    "pluralName": "categories",
    "displayName": "category"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "categoryName": {
      "type": "string",
      "required": true,
      "default": "Category Name"
    },
    "categorySlug": {
      "type": "uid",
      "targetField": "categoryName",
      "required": true
    },
    "product": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product.product",
      "inversedBy": "categories"
    }
  }
}
